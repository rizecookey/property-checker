ident inv;

annotation InvUnknown() any :<==> "true" for "true";
annotation Inv() java.lang.Object :<==> "§subject§ != null ==> \\invariant_for(§subject§)" for "true";
annotation InvBottom() any :<==> "false" for "true";

relation InvBottom() <: InvUnknown() :<==> "true";
relation InvBottom() <: Inv() :<==> "true";
relation Inv() <: InvUnknown() :<==> "true";
